{
  "name": "@univision/fe-commons",
  "version": "10.680.2",
  "description": "Common utility components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/televisa-univision/univision-fe.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "start": "npm-run-all lint jest storybook",
    "docs": "esdoc",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watch",
    "test:skip": "npm run jest:parallel",
    "jest": "NODE_ENV=test jest --coverage --runInBand --colors",
    "jest:parallel": "jest --coverage",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:src lint:test",
    "lint:src": "eslint --config ../../.eslintrc 'src/**/*.js' --ignore-pattern 'src/**/*.spec.js'",
    "lint:test": "eslint --config ../../.eslintrc.spec 'src/**/*.spec.js'",
    "storybook": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6007",
    "clean": "rimraf dist",
    "export": "build-storybook -c .storybook -o dist/story",
    "babel": "npm-run-all babel:esm babel:cjs",
    "babel:esm": "TARGET=esm babel src --out-dir esm --copy-files --no-copy-ignored",
    "babel:cjs": "TARGET=cjs babel src --out-dir dist --copy-files --no-copy-ignored",
    "build": "npm-run-all clean babel build:post",
    "build:post": "rimraf '+(dist|esm)/**/+(__tests__|__mocks__|*.stories.*)'",
    "build:watch": "npm-run-all 'babel --watch'"
  },
  "author": "UVN Platform Engineers",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@reduxjs/toolkit": "^1.3.6",
    "@univision/fe-graphql-services": "^10.680.1",
    "@univision/fe-utilities": "^10.680.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "healthcheck-ping": "^1.1.0",
    "helmet": "^3.20.0",
    "history": "^4.9.0",
    "jwt-decode": "2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "mkdirp": "^0.5.1",
    "next": "^10.0.4",
    "npm-run-all": "^4.1.5",
    "perfume.js": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "^5.2.1",
    "react-lazyload": "2.6.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-timeago": "^4.4.0",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.5",
    "serve-favicon": "^2.5.0",
    "style-loader": "^0.23.1",
    "styled-components": "^5.0.1",
    "ua-parser-js": "^0.7.21",
    "url": "^0.11.0",
    "url-loader": "^1.1.2",
    "whatwg-fetch": "^2.0.4",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-info": "4.1.18",
    "@storybook/addon-links": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "@testing-library/react-hooks": "^3.2.1",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esdoc": "^1.1.0",
    "eslint": "6.5.1",
    "gpt-mock": "^1.0.2",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-transform-graphql": "2.1.0",
    "nodemon": "^1.18.10",
    "react-test-renderer": "16.13.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^2.6.2",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^@univision\\/([^/]+)/(?!(dist|src|esm))(.*)": "@univision/$1/esm/$3",
      "^assets(.*)$": "<rootDir>/assets$1",
      "^components(.*)$": "<rootDir>/components$1",
      "^config(.*)$": "<rootDir>/config$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/esm/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/doc/",
      "<rootDir>/dist/",
      "<rootDir>/esm/",
      "<rootDir>/src/config/",
      "<rootDir>/src/themes/",
      "<rootDir>/src/utils/ads/vendors",
      "<rootDir>/src/utils/images/dims"
    ],
    "setupFiles": [
      "<rootDir>/config/testsConfig.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global"
  }
}
