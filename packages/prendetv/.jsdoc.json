{
  "opts": {
    "destination": "dist/docs",
    "recurse": true,
    "readme": "./README.md",
    "template": "../../node_modules/docdash"
  },
  "plugins": [
    "plugins/sourcetag",
    "plugins/markdown",
    "./docs/plugins/category"
  ],
  "recurseDepth": 20,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "((^|\\/|\\\\)_|.+\\.stories.*|.+\\.spec.*|.+\\.styles.*)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Mixins",
      "Events",
      "Interfaces",
      "Namespaces",
      "Externals",
      "Tutorials"
    ],
    "search": true,
    "collapse": true,
    "wrap": true,
    "typedefs": true,
    "menu": {
      "Github": {
        "href":"https://github.com/televisa-univision/univision-fe/tree/master/packages/fe-prendetv",
        "target":"_blank"
      },
      "Storybook": {
        "href":"https://univision-fe-integration.k8s.dev-univision.com/storybook/fe-prendetv",
        "target":"_blank"
      }
    }
  }
}
