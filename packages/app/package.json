{
  "private": true,
  "name": "@univision/fe-webapp",
  "version": "10.682.0",
  "description": "Main app for Univision sites",
  "author": "UVN Platform Engineers",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/televisa-univision/univision-fe.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "start": "yarn -r dotenv/config next start",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watch",
    "test:skip": "npm run jest:parallel",
    "jest": "NODE_ENV=test jest --coverage --runInBand --colors",
    "jest:parallel": "jest --coverage",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:src lint:test",
    "lint:src": "eslint --config .eslintrc.js './**/*.js' --ignore-pattern '*.spec.js' --ignore-pattern '*.stories.js'",
    "lint:test": "eslint --config .eslintrc.spec './**/*.spec.js' './**/*.stories.js'",
    "storybook": "start-storybook -p 6011",
    "export": "build-storybook -c .storybook -o dist/story",
    "clean": "rimraf build && rimraf dist",
    "app:dev": "NODE_OPTIONS='--max_old_space_size=4096 --openssl-legacy-provider' next dev",
    "app:prod": "npm-run-all app:build start",
    "app:debug": "npm-run-all build:sourcemap start",
    "app:build": "next build --profile",
    "app:inspect": "node --inspect node_modules/.bin/next dev",
    "build:analyze": "NODE_OPTIONS='--max_old_space_size=4096' ANALYZE=true next build",
    "build:serverless": "SERVERLESS=true next build --profile",
    "build:stats": "STATS=true next build",
    "build:debug": "DEBUG=true next build --profile",
    "build:sourcemap": "SOURCEMAP=true next build --profile",
    "deploy": "SLS_DEBUG=* node node_modules/.bin/serverless deploy --verbose --stage"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs2": "^7.8.7",
    "@univision/fe-commons": "^10.680.2",
    "@univision/fe-components-base": "^10.682.0",
    "@univision/fe-deportes": "^10.682.0",
    "@univision/fe-icons": "^10.680.1",
    "@univision/fe-local": "^10.682.0",
    "@univision/fe-prendetv": "^10.680.2",
    "@univision/fe-video": "^10.682.0",
    "@univision/fmg-video-sdk": "4.459.0-beta.11",
    "@univision/shared-components": "0.5.152-rc.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "ignore-loader": "^0.1.2",
    "intersection-observer": "^0.11.0",
    "lodash.debounce": "^4.0.8",
    "native-url": "^0.2.6",
    "next": "^10.0.4",
    "next-compose-plugins": "^2.2.0",
    "next-server": "^9.0.5",
    "next-transpile-modules": "^4.0.2",
    "next-videos": "1.1.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-lazyload": "^2.6.7",
    "react-native-web": "0.9.13",
    "react-redux": "^7.2.0",
    "react-swipeable": "^4.3.2",
    "react-timeago": "^4.4.0",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.0.5",
    "sanitize-html": "^2.3.3",
    "sass": "^1.64.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^9.5.2",
    "@storybook/addon-info": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "@testing-library/react-hooks": "^3.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esdoc": "^1.1.0",
    "eslint": "^6.5.1",
    "jest": "^26.6.3",
    "jest-next-dynamic": "^1.0.1",
    "rimraf": "^2.6.2",
    "serverless": "^3.35.2",
    "serverless-nextjs-plugin": "^2.4.5",
    "serverless-plugin-canary-deployments": "^0.4.8",
    "serverless-plugin-datadog": "^0.26.0",
    "serverless-prune-plugin": "^1.4.3",
    "stats-webpack-plugin": "^0.7.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpackbar": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(txt)$": "<rootDir>/__mocks__/rawLoaderTransform"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@univision/|react-(.*)))"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|ico|eot|otf|webp|svg|ttf|woff|woff2|mp4)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^@univision\\/([^/]+)/(?!(dist|src|esm))(.*)": "@univision/$1/esm/$3"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/doc/",
      "<rootDir>/build/"
    ],
    "setupFiles": [
      "<rootDir>/config/tests.env.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/tests.setup.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global"
  }
}
