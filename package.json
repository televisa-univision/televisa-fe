{
  "private": true,
  "versionType": "patch",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/immer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/univision/univision-fe.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "prepublishOnly": "NODE_ENV=production npm-run-all build",
    "build": "NODE_ENV=production next build",
    "start": "yarn run:core start -s",
    "start:dev": "yarn run:core dev",
    "start:dev:inspect": "yarn run:core dev:inspect",
    "start:prod": "yarn run:core prod",
    "start:app:dev": "yarn run:app app:dev",
    "start:app:prod": "yarn run:app app:prod",
    "start:app:inspect": "yarn run:app app:inspect",
    "start:commons": "yarn run:commons start",
    "start:components-base": "yarn run:components-base start",
    "start:local": "yarn run:local start",
    "start:icons": "yarn run:icons start",
    "start:icons:native": "yarn run:icons start:native",
    "start:deportes": "yarn run:deportes start",
    "start:vix": "yarn run:vix start",
    "start:utilities": "yarn run:utilities start",
    "run:app": "cd packages/app && yarn",
    "run:ci": "cd packages/tools/ci && yarn",
    "run:core": "cd packages/core && yarn",
    "run:commons": "cd packages/commons && yarn",
    "run:components-base": "cd packages/components-base && yarn",
    "run:graphql-services": "cd packages/graphql-services && yarn",
    "run:local": "cd packages/local && yarn",
    "run:icons": "cd packages/icons && yarn",
    "run:icons:ios": "yarn run:icons run:ios",
    "run:icons:android": "yarn run:icons run:android",
    "run:deportes": "cd packages/deportes && yarn",
    "run:vix": "cd packages/vix && yarn",
    "run:prendetv": "cd packages/prendetv && yarn",
    "run:tools": "cd packages/tools && yarn",
    "run:video": "cd packages/video && yarn",
    "run:utilities": "cd packages/utilities && yarn",
    "run:e2e": "wait-on http://localhost:8080/ && yarn run:e2e:all",
    "run:e2e:all": "cd packages/end-to-end && yarn cypress:run:all",
    "run:e2e:desktop": "cd packages/end-to-end && yarn cypress:run:desktop",
    "run:e2e:mobile": "cd packages/end-to-end && yarn cypress:run:mobile",
    "open:e2e:desktop": "cd packages/end-to-end && yarn cypress:open:desktop",
    "open:e2e:mobile": "cd packages/end-to-end && yarn cypress:open:mobile",
    "build:all": "npm-run-all build:commons build:components-base build:local build:icons build:deportes build:vix build:video build:graphql-services build:utilities build:prendetv",
    "build:all:watch": "npm-run-all --parallel build:commons:watch build:components-base:watch build:local:watch build:icons:watch build:deportes:watch build:vix:watch build:video:watch build:graphql-services:watch build:utilities:watch build:prendetv:watch",
    "build:app": "yarn run:app app:build",
    "build:commons": "yarn run:commons build",
    "build:commons:watch": "yarn run:commons build:watch",
    "build:components-base": "yarn run:components-base build",
    "build:components-base:watch": "yarn run:components-base build:watch",
    "build:graphql-services": "yarn run:graphql-services build",
    "build:graphql-services:watch": "yarn run:graphql-services build:watch",
    "build:local": "yarn run:local build",
    "build:local:watch": "yarn run:local build:watch",
    "build:icons": "yarn run:icons build",
    "build:icons:watch": "yarn run:icons build:watch",
    "build:deportes": "yarn run:deportes build",
    "build:vix": "yarn run:vix build",
    "build:deportes:watch": "yarn run:deportes build:watch",
    "build:vix:watch": "yarn run:vix build:watch",
    "build:prendetv": "yarn run:prendetv build",
    "build:prendetv:watch": "yarn run:prendetv build:watch",
    "build:utilities": "yarn run:utilities build",
    "build:utilities:watch": "yarn run:utilities build:watch",
    "build:video": "yarn run:video build",
    "build:video:watch": "yarn run:video build:watch",
    "clean:all": "npm-run-all --continue-on-error --parallel clean:app clean:commons clean:components-base clean:local clean:icons clean:core clean:deportes clean:vix clean:prendetv clean:video clean:graphql-services",
    "clean:app": "yarn run:app run clean",
    "clean:commons": "yarn run:commons run clean",
    "clean:components-base": "yarn run:components-base run clean",
    "clean:graphql-services": "yarn run:graphql-services run clean",
    "clean:local": "yarn run:local run clean",
    "clean:icons": "yarn run:icons run clean",
    "clean:core": "yarn run:core run clean",
    "clean:deportes": "yarn run:deportes run clean",
    "clean:vix": "yarn run:vix run clean",
    "clean:prendetv": "yarn run:prendetv run clean",
    "clean:utilities": "yarn run:utilities run clean",
    "clean:video": "yarn run:video run clean",
    "clean:jestCache": "yarn jest --showConfig | grep 'cacheDirectory' | grep -o '\"/\\<[A-Za-z].*\\>\"' | xargs rm -rf",
    "clean:lerna:build": "lerna clean --yes && yarn && yarn build:all",
    "docs:all": "npm-run-all --continue-on-error --parallel docs:utilities",
    "docs:core": "yarn run:core docs",
    "docs:utilities": "yarn run:utilities docs",
    "test:all": "npm-run-all --aggregate-output --continue-on-error --parallel --max-parallel 5 test:app test:commons test:components-base test:core test:local test:icons test:deportes test:vix test:video test:graphql-services test:utilities",
    "test:app": "yarn run:app test",
    "test:commons": "yarn run:commons test",
    "test:commons:watch": "yarn run:commons test:watch",
    "test:components-base": "yarn run:components-base test",
    "test:components-base:watch": "yarn run:components-base test:watch",
    "test:graphql-services": "yarn run:graphql-services test",
    "test:graphql-services:watch": "yarn run:graphql-services test:watch",
    "test:local": "yarn run:local test",
    "test:icons": "yarn run:icons test",
    "test:core": "yarn run:core test",
    "test:core:watch": "yarn run:core test:watch",
    "test:deportes": "yarn run:deportes test",
    "test:vix": "yarn run:vix test",
    "test:prendetv": "yarn run:prendetv test",
    "test:utilities": "yarn run:utilities test",
    "test:video": "yarn run:video test",
    "lint:all": "npm-run-all --aggregate-output --continue-on-error --parallel --max-parallel 5 lint:app lint:commons lint:components-base lint:core lint:local lint:icons lint:deportes lint:vix lint:prendetv lint:video lint:graphql-services lint:utilities",
    "lint:app": "yarn run:app lint",
    "lint:commons": "yarn run:commons lint",
    "lint:components-base": "yarn run:components-base lint",
    "lint:graphql-services": "yarn run:graphql-services lint",
    "lint:local": "yarn run:local lint",
    "lint:icons": "yarn run:icons lint",
    "lint:core": "yarn run:core lint",
    "lint:deportes": "yarn run:deportes lint",
    "lint:vix": "yarn run:vix lint",
    "lint:prendetv": "yarn run:prendetv lint",
    "lint:utilities": "yarn run:utilities lint",
    "lint:video": "yarn run:video lint",
    "story:app": "yarn run:app storybook",
    "story:core": "yarn run:core storybook",
    "story:commons": "yarn run:commons storybook",
    "story:components-base": "yarn run:components-base storybook",
    "story:local": "yarn run:local storybook",
    "story:icons": "yarn run:icons storybook",
    "story:icons:native": "yarn run:icons storybook:native",
    "story:deportes": "yarn run:deportes storybook",
    "story:vix": "yarn run:vix storybook",
    "story:prendetv": "yarn run:prendetv storybook",
    "story:utilities": "yarn run:utilities storybook",
    "story:video": "yarn run:video storybook",
    "story:export:all": "npm-run-all --aggregate-output --continue-on-error --parallel --max-parallel $([ -z $MAX_PARALLEL ] && echo 5 || echo $MAX_PARALLEL) story:export:commons story:export:components-base story:export:local story:export:icons story:export:deportes story:export:vix story:export:video story:export:utilities",
    "story:export:commons": "yarn run:commons export",
    "story:export:components-base": "yarn run:components-base export",
    "story:export:core": "yarn run:core export",
    "story:export:local": "yarn run:local export",
    "story:export:icons": "yarn run:icons export",
    "story:export:deportes": "yarn run:deportes export",
    "story:export:vix": "yarn run:vix export",
    "story:export:prendetv": "yarn run:prendetv export",
    "story:export:utilities": "yarn run:utilities export",
    "story:export:video": "yarn run:video export",
    "tools:performance:entretenimiento": "yarn run:tools performance:entretenimiento",
    "tools:performance:deportes": "yarn run:tools performance:deportes",
    "tools:performance:vix": "yarn run:tools performance:vix",
    "tools:performance:prendetv": "yarn run:tools performance:prendetv",
    "ci:cancelBuilds": "yarn run:tools ci:cancel",
    "ci:docs": "yarn run:tools ci:docs",
    "ci:setEnvs": "yarn run:tools ci:setEnvs",
    "ci:tagRelease": "yarn run:tools ci:tagRelease",
    "ci:monitor": "yarn run:tools ci:monitor",
    "ci:storybooks": "yarn run:tools ci:storybooks"
  },
