{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "cypress/globals": true
  },
  "plugins": [
    "cypress"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended"
    ],
  "rules": {
    "no-console": "off",
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
