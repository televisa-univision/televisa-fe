{
  "private": true,
  "name": "@univision/fe-tools-ci",
  "version": "8.0.20",
  "description": "Univision FE CI Scripts.",
  "repository": {
    "type": "git",
    "url": "https://github.com/univision/univision-fe.git"
  },
  "engines": {
    "node": "<=9.11.2"
  },
  "scripts": {
    "ci:cancel": "node ./cancelRedundantJobs",
    "ci:docs": "node ./docs/run",
    "ci:setEnvs": "node -e \"require('./deploy').prepareEnvs()\"",
    "ci:tagRelease": "node -e \"require('./tagRelease').tagRelease()\"",
    "ci:uploadAssets": "node ./assets/upload",
    "ci:monitor": "node ./monitoring/run",
    "ci:storybooks": "node ./storybooks/run"
  },
  "author": "UVN Platform Engineers",
  "license": "MIT",
  "dependencies": {
    "@univision/fe-utilities": "^10.244.0",
    "aws-sdk": "^2.430.0",
    "mime-types": "^2.1.22",
    "recursive-readdir": "^2.2.2",
    "simple-git": "^1.116.0",
    "psi": "^4.0.1",
    "chalk": "^4.1.0"
  }
}
