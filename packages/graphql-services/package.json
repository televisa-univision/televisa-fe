{
  "name": "@univision/fe-graphql-services",
  "version": "10.680.1",
  "description": "GraphQL services, queries and mutations schemas",
  "keywords": [
    "graphql",
    "services",
    "gql",
    "queries",
    "mutations",
    "univision"
  ],
  "homepage": "https://github.com/televisa-univision/univision-fe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/televisa-univision/univision-fe.git"
  },
  "license": "MIT",
  "author": "UVN Platform Engineers",
  "scripts": {
    "babel": "babel src --out-dir dist",
    "build": "npm-run-all clean babel",
    "build:watch": "npm-run-all 'babel --watch'",
    "clean": "rimraf dist",
    "docs": "esdoc",
    "schemas": "node ./scripts/index.js",
    "jest": "NODE_ENV=test jest --coverage --runInBand --colors",
    "jest:parallel": "jest --coverage",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:src lint:test",
    "lint:src": "eslint --config ../../.eslintrc 'src/**/*.js' --ignore-pattern 'src/**/*.spec.js'",
    "lint:test": "eslint --config ../../.eslintrc.spec 'src/**/*.spec.js'",
    "test": "npm run jest",
    "test:skip": "npm run jest:parallel",
    "test:watch": "npm run jest -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "casual-browserify": "^1.5.19-2",
    "dotenv": "^6.2.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "eslint": "^6.5.1",
    "graphql-tag": "^2.10.3",
    "jest": "^26.6.3",
    "jest-transform-graphql": "^2.1.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/config/",
      "<rootDir>/doc/",
      "<rootDir>/dist/",
      "<rootDir>/serviceMock/schemas"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "moduleNameMapper": {
      "^config(.*)$": "<rootDir>/config$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupFilesAfterEnv.js"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.js$": "babel-jest"
    }
  }
}
