{
  "private": true,
  "name": "univision-fe",
  "version": "10.682.0",
  "description": "Universal React Web App",
  "repository": {
    "type": "git",
    "url": "https://github.com/univision/univision-fe.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "start": "npm run prod:server:rendering",
    "docs": "esdoc",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watch",
    "test:skip": "npm run jest:parallel",
    "jest": "NODE_ENV=test jest --coverage --runInBand --colors",
    "jest:parallel": "jest --coverage",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:src lint:test",
    "lint:src": "eslint --config ../../.eslintrc 'src/**/*.js' --ignore-pattern 'src/**/*.spec.js'",
    "lint:test": "eslint --config ../../.eslintrc.spec 'src/**/*.spec.js'",
    "storybook": "NODE_OPTIONS='--openssl-legacy-provider' start-storybook -p 6011",
    "export": "build-storybook -c .storybook -o dist/out",
    "clean": "rimraf ./build",
    "dev": "npm-run-all clean dev:prepare dev:build",
    "dev:inspect": "npm-run-all clean dev:prepare dev:build:inspect",
    "yarn ": "npm-run-all clean dev:prepare dev:build:inspect",
    "dev:prepare": "universal-webpack --settings ./webpack/universalWebpackSettings.json --config ./webpack/webpack.config.js prepare",
    "dev:build": "NODE_ENV=development npm-run-all --parallel dev:client:build dev:server:rendering:build dev:server:rendering",
    "dev:build:inspect": "NODE_ENV=development npm-run-all --parallel dev:client:build dev:server:rendering:build dev:server:rendering:inspect",
    "dev:client:build": "webpack-serve --hot-client --require @babel/register --config ./webpack/webpack.config.client.development.babel.js",
    "dev:server:rendering:build": "webpack --mode development --watch --colors --display-error-details --hide-modules --config ./webpack/webpack.config.server.development.babel.js",
    "dev:server:rendering": "nodemon --quiet ./src/server/entry.js --watch ./src/server --watch ./build/server --ignore 'src/**/*.spec.js' --ignore __mocks__ --no-update-notifier",
    "dev:server:rendering:inspect": "nodemon --quiet --inspect ./src/server/entry.js --watch ./src/server --watch ./build/server --ignore 'src/**/*.spec.js' --ignore __mocks__ --no-update-notifier",
    "delay": "node ./scripts/sleep 7000",
    "prod": "NODE_ENV=production npm-run-all prod:build prod:start",
    "prod:prepare": "universal-webpack --settings ./webpack/universalWebpackSettings.json --config ./webpack/webpack.config.js prepare",
    "prod:server": "NODE_ENV=production npm-run-all prod:server:rendering:build prod:start",
    "prod:start": "npm run prod:server:rendering",
    "prod:build": "npm-run-all --max-old-space-size=2000 --parallel prod:prepare prod:client:build prod:server:rendering:build",
    "prod:client:build": "webpack --mode production --colors --display-error-details --hide-modules --config ./webpack/webpack.config.client.production.babel.js",
    "prod:server:rendering:build": "webpack --mode production --colors --display-error-details --hide-modules --config ./webpack/webpack.config.server.production.babel.js",
    "prod:server:rendering": "NODE_ENV=production forever ./src/server/entry.js",
    "analyzer": "cross-env ANALYZER=\"server\" npm run dev",
    "audit:widgets:list": "babel-node --plugins dynamic-import-node ./scripts/widgets/list"
  },
  "author": "UVN Platform Engineers",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@univision/fe-commons": "^10.680.2",
    "@univision/fe-components-base": "^10.682.0",
    "@univision/fe-deportes": "^10.682.0",
    "@univision/fe-icons": "^10.680.1",
    "@univision/fe-local": "^10.682.0",
    "@univision/fe-utilities": "^10.680.1",
    "@univision/fe-video": "^10.682.0",
    "@univision/fmg-video-sdk": "4.461.1-rc.0",
    "@univision/shared-components": "0.5.152-rc.2",
    "basic-auth": "^2.0.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cors": "^2.8.5",
    "css-loader": "^1.0.1",
    "dd-trace": "^0.9.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "forever": "^0.15.3",
    "healthcheck-ping": "^1.1.0",
    "helmet": "^3.20.0",
    "helmet-csp": "^2.8.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest-transform-graphql": "^2.1.0",
    "le_node": "^1.8.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "mini-css-extract-plugin": "^0.4.5",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "on-finished": "^2.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "purify-css": "^1.2.5",
    "raw-loader": "^0.5.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.19",
    "react-lazyload": "2.6.2",
    "react-loadable": "^5.5.0",
    "react-native-web": "0.9.13",
    "react-redux": "^7.2.0",
    "react-slick": "^0.25.2",
    "react-sticky": "^6.0.3",
    "react-swipeable": "^4.3.2",
    "react-timeago": "^3.4.3",
    "react-visibility-sensor": "^5.1.1",
    "recursive-readdir": "^2.2.2",
    "rimraf": "^2.6.2",
    "sanitize-html": "^2.3.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.5",
    "serialize-javascript": "^3.1.0",
    "serve-favicon": "^2.5.0",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.23.1",
    "styled-components": "^5.0.1",
    "terser-webpack-plugin": "^2.1.0",
    "tsscmp": "^1.0.6",
    "universal-webpack": "^0.6.11",
    "url-loader": "^1.1.2",
    "uuid": "^2.0.3",
    "webpack": "^4.40.3",
    "webpack-cli": "^3.3.9",
    "webpack-serve": "^2.0.3",
    "whatwg-fetch": "^2.0.4",
    "winston": "^3.9.0",
    "workbox-webpack-plugin": "^4.2.0",
    "workbox-window": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-info": "4.1.18",
    "@storybook/addon-links": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esdoc": "^1.1.0",
    "eslint": "6.5.1",
    "gpt-mock": "^1.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "nodemon": "^1.18.10",
    "react-test-renderer": "16.13.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@univision/|react-(.*)))"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^@univision\\/([^/]+)/(?!(dist|src|esm))(.*)": [
        "@univision/$1/esm/$3",
        "@univision/$1/$3"
      ],
      "^server(.*)$": "<rootDir>/src/server$1",
      "^app(.*)$": "<rootDir>/src/app$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^components(.*)$": "<rootDir>/src/app/components$1"
    },
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/src/app/config.js",
      "<rootDir>/src/server/config.js",
      "<rootDir>/doc/",
      "<rootDir>/src/app/utils/nodeDetector.js",
      "<rootDir>/src/app/utils/ads/vendors/",
      "<rootDir>/src/app/utils/logging/serverLogger.js",
      "<rootDir>/src/server/proxy/api/page/PageApi.js"
    ],
    "setupFiles": [
      "<rootDir>/config/testsConfig.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global"
  }
}
